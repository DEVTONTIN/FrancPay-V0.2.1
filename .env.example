# Database connection used by Prisma & migrations (service role recommended for CI)
DATABASE_URL="postgresql://postgres:[SUPABASE_SERVICE_PASSWORD]@db.[PROJECT_REF].supabase.co:5432/postgres"
SUPABASE_SERVICE_ROLE_KEY="[SERVICE_ROLE_KEY]"

# Client-side Supabase configuration (public anon key)
VITE_SUPABASE_URL="https://[PROJECT_REF].supabase.co"
VITE_SUPABASE_ANON_KEY="[PUBLIC_ANON_KEY]"

# TonConnect auth service
TONCONNECT_APP_DOMAIN="app.francpay.com"
TON_WALLET_SESSION_TTL_SECONDS="86400"
